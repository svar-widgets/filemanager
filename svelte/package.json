{
	"name": "wx-svelte-filemanager",
	"version": "2.1.0",
	"description": "Easy-to-use Svelte component for adding a file manager (file explorer) UI",
	"productTag": "filemanager",
	"productTrial": false,
	"type": "module",
	"scripts": {
		"build": "vite build",
		"build:dist": "vite build --mode dist",
		"build:tests": "vite build --mode test",
		"lint": "yarn eslint ./demos ./src",
		"start": "vite --open",
		"start:tests": "vite --open=/tests/ --host 0.0.0.0  --port 5100 --mode test",
		"test": "true",
		"test:cypress": "cypress run -P ./ --config \"baseUrl=http://localhost:5100/tests\""
	},
	"svelte": "src/index.js",
	"exports": {
		".": {
			"svelte": "./src/index.js"
		},
		"./package.json": "./package.json"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/svar-widgets/filemanager.git"
	},
	"bugs": {
		"url": "https://forum.svar.dev"
	},
	"homepage": "https://svar.dev/svelte/filemanager/",
	"dependencies": {
		"wx-filemanager-data-provider": "2.1.0",
		"wx-filemanager-locales": "2.1.0",
		"wx-filemanager-store": "2.1.0",
		"wx-lib-dom": "0.8.0",
		"wx-lib-svelte": "0.5.1",
		"wx-lib-state": "1.9.0",
		"wx-svelte-core": "2.1.0",
		"wx-svelte-menu": "2.1.0",
		"wx-svelte-grid": "2.1.0",
		"wx-svelte-uploader": "2.1.0"
	},
	"files": [
		"src",
		"readme.md",
		"whatsnew.md",
		"license.txt"
	],
	"keywords": [
		"svelte",
		"file manager",
		"file-manager",
		"file-explorer",
		"file explorer",
		"file browser",
		"svelte component",
		"ui component",
		"svelte file manager",
		"svar widgets"
	]
}
